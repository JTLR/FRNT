{
    "name": "FRNT",
    "version": "2.0.0",
    "description": "NPM dependencies for FRNT frontend framework",
    "author": "Joe Taylor",
    "repository": {
        "type": "git",
        "url": "https://github.com/JTLR/FRNT"
    },
    "scripts": {
        "precommit": "lint-staged",
        "build": "webpack -p",
        "watch": "webpack --watch"
    },
    "lint-staged": {
        "*.{js,json,css,md,ts,scss}": [
            "prettier --write --tab-width 4",
            "git add"
        ],
        "src/frontend/**/*.scss": [
            "node automation/update-cache-busting.js css",
            "git add"
        ]
    },
    "dependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "bootstrap": "^4.1.1",
        "core-js": "^2.5.7",
        "grunt": "^1.0.3",
        "grunt-autoprefixer": "^3.0.4",
        "grunt-cli": "^1.2.0",
        "grunt-combine-media-queries": "^1.0.20",
        "grunt-contrib-clean": "^1.1.0",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-cssmin": "^2.2.1",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-notify": "^0.4.5",
        "grunt-run": "^0.8.1",
        "grunt-sass": "2.1.0",
        "grunt-webpack": "^3.1.2",
        "husky": "^0.14.3",
        "jsonfile": "^4.0.0",
        "lint-staged": "^7.2.0",
        "node-bourbon": "^4.2.8",
        "prettier": "^1.13.7",
        "webpack": "^4.14.0",
        "webpack-cli": "^3.0.8"
    }
}
